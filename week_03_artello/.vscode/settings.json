{
	"dart.flutterSdkPath": ".fvm/flutter_sdk",
	// Remove .fvm files from search
	"search.exclude": {
		"**/.fvm": true
	},
	// Remove from file watching
	"files.watcherExclude": {
		"**/.fvm": true
	},
	"cSpell.words": [
		"appbar",
		"artboard",
		"artello",
		"changa",
		"Getx",
		"lerp",
		"LTWH",
		"Reclip",
		"screenutil",
		"Somen",
		"squircle",
		"vsync",
		"wakelock",
		"webp"
	],
	"java.configuration.updateBuildConfiguration": "interactive"
}